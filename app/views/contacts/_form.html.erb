<% form_for @contact do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label :atividade %><br />
    <%= f.text_field :atividade %>
  </p>
  <p>
    <%= f.label :organizacao %><br />
    <%= f.text_field :organizacao %>
  </p>
  <p>
    <%= f.label :endereco %><br />
    <%= f.text_field :endereco %>
  </p>
  <p>
    <%= f.label :telefone %><br />
    <%= f.text_field :telefone %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :data_nascimento %><br />
    <%= f.text_field :data_nascimento %>
  </p>
  <p>
    <%= f.label :interesses %><br />
    <%= f.text_area :interesses %>
  </p>
  <p>
    <%= f.label :personalidade %><br />
    <%= f.text_area :personalidade %>
  </p>
  <p>
    <%= f.label :acompanhamento %><br />
    <%= f.text_area :acompanhamento %>
  </p>
  <p><%= f.submit "Gravar" %></p>
<% end %>
